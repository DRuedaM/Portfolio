---
import ExperienceItem from "./ExperienceItem.astro"
const EXPERIENCE = [
    {
        date:  'March 2024 - June 2024',
        title: 'Internship developer',
        description: 'Internship employee in the develop department',
    },
    {
        date:  'March 2022 - June 2022',
        title: 'Internship systems',
        description: 'Internship in the department of technology in Windows systems and user support',
    },
]
---
<ol class="relative border-s border-gray-200 dark:border-gray-700 ml-3">         
    {
        EXPERIENCE.map(experience => 
        (
            <li class="mb-10 ms-4">
                <ExperienceItem {...experience}/>
            </li>
        ))
    }
</ol>

